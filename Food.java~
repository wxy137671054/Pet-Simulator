public class Food extends Product{
   
   public void Food(int pID, String pName, int pCost, int pValue){
      productID = pID;
      productName = pName;
      productCost = pCost;
      productValue = pValue;
   }
   
   //Accessor
   public int getProductID(){
      return productID;
   }
   
   public String getProductName(){
      return productName;
   }
   
   public int getProductCost(){
      return productCost;
   }
   
   public int getProductValue(){
      return productValue;
   }
   
}